  <xsl:template match="/cda:ClinicalDocument" priority="4000" mode="M6">
    <xsl:choose>
      <xsl:when test="cda:author" />
      <xsl:otherwise>
        <xsl:element name="issue">
          <xsl:element name="message">Error: e-Discharge Summary - 6.1.1 Document Author - The 'Author' tag is missing. Refer to section 6.1.1 Document Author of the e-Discharge_Summary_CDA_Implementation_Guide_v3.4.</xsl:element>
