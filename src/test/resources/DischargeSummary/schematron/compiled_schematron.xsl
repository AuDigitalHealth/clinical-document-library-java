    </xsl:if>
    <xsl:apply-templates mode="M6" />
  </xsl:template>
  <xsl:template match="/cda:ClinicalDocument/cda:author" priority="3999" mode="M6">
    <xsl:choose>
      <xsl:when test="cda:time" />
      <xsl:otherwise>
        <xsl:element name="issue">
          <xsl:element name="message">Error: e-Discharge Summary 6.1.1 Document Author - "Document Author / Participation Type" - The 'time' tag is missing. Refer to section 6.1.1 Document Author of the e-Discharge_Summary_CDA_Implementation_Guide_v3.4.</xsl:element>
