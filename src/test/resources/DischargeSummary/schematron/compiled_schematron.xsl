            <xsl:apply-templates select="." mode="schematron-get-full-path" />
          </xsl:element>
          <xsl:element name="test">cda:time</xsl:element>
        </xsl:element>
      </xsl:otherwise>
    </xsl:choose>
    <xsl:if test="count(cda:time) &gt; 1">
      <xsl:element name="issue">
        <xsl:element name="message">Error: e-Discharge Summary 6.1.1 Document Author - "Document Author / Participation Period" - The 'time' tag shall appear only once. Refer to section 6.1.1 Document Author of the e-Discharge_Summary_CDA_Implementation_Guide_v3.4.</xsl:element>
        <xsl:element name="context">
          <xsl:apply-templates select="." mode="schematron-get-full-path" />
        </xsl:element>
        <xsl:element name="test">count(cda:time) &gt; 1</xsl:element>
      </xsl:element>
    </xsl:if>
    <xsl:if test="(@typeCode) and normalize-space(@typeCode) = ''">
      <xsl:element name="issue">
        <xsl:element name="message">Error: e-Discharge Summary - 6.1.1 Document Author - "Document Author / Participation Type" - The 'author' tag 'typeCode' attribute shall contain a value. Refer to section 6.1.1 Document Author of the e-Discharge_Summary_CDA_Implementation_Guide_v3.4.</xsl:element>
        <xsl:element name="context">
          <xsl:apply-templates select="." mode="schematron-get-full-path" />
        </xsl:element>
        <xsl:element name="test">(@typeCode) and normalize-space(@typeCode) = ''</xsl:element>
      </xsl:element>
    </xsl:if>
    <xsl:if test="(@typeCode) and normalize-space(@typeCode) != '' and @typeCode != 'AUT'">
      <xsl:element name="issue">
        <xsl:element name="message">Error: e-Discharge Summary - 6.1.1 Document Author - "Document Author / Participation Type" - The 'author' tag 'typeCode' attribute shall contain the value 'AUT'. Refer to section 6.1.1 Document Author of the e-Discharge_Summary_CDA_Implementation_Guide_v3.4.</xsl:element>
        <xsl:element name="context">
          <xsl:apply-templates select="." mode="schematron-get-full-path" />
        </xsl:element>
        <xsl:element name="test">(@typeCode) and normalize-space(@typeCode) != '' and @typeCode != 'AUT'</xsl:element>
