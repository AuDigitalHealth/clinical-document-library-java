            <xsl:apply-templates select="." mode="schematron-get-full-path" />
          </xsl:element>
          <xsl:element name="test">cda:author</xsl:element>
        </xsl:element>
      </xsl:otherwise>
    </xsl:choose>
    <xsl:if test="count(cda:author) &gt; 1">
      <xsl:element name="issue">
        <xsl:element name="message">Error: e-Discharge Summary - 6.1.1 Document Author - The 'author' tag shall appear only once. Refer to section 6.1.1 Document Author of the e-Discharge_Summary_CDA_Implementation_Guide_v3.4.</xsl:element>
