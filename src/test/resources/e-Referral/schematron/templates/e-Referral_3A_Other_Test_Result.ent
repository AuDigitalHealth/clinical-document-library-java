

    <!-- e-Referral Clinical Document:  Other Test Result -->

    <!-- Context: /ClinicalDocument/component/structuredBody/component[diag_inv]/section/ - Other Test Result -->

    <!-- Status: Last Reviewed: 
         Status: Last Updated : 3/12/2012 -->


    <pattern name="p-e-Referral_3A_Other_Test_Result-errors"
        id="p-e-Referral_3A_Other_Test_Result-errors"
        see="#p-e-Referral_3A_Other_Test_Result-errors">

        <rule
            context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '101.20117']/cda:component/cda:section[cda:code/@code = '102.16029']">

            <!-- Other Test Result - 1..1 -->

            <assert test="cda:entry[cda:observation/@classCode='OBS']">Error: e-Referral - Other
                (Pathology or radiology) Test Result - - The 'entry' tag is missing. Refer to section
                of the e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <report test="count(cda:entry[cda:observation/@classCode='OBS']) > 1">Error: e-Referral
                - Other (Pathology or radiology) Test Result - The 'entry' tag shall appear
                only once. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>

        </rule>

        <rule
            context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '101.20117']/cda:component/cda:section[cda:code/@code = '102.16029']/cda:entry[cda:observation/@classCode='OBS']">

            <!-- OTHER TEST RESULT NAME - 1..1 -->


            <report test="count(cda:observation) > 1">Error: e-Referral - Other (Pathology or
                radiology) Test Result - The 'observation' tag shall appear only once. Refer to
                section  of the e-Referral_CDA_Implementation_Guide_v2.2.</report>

        </rule>

        <rule
            context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '101.20117']/cda:component/cda:section[cda:code/@code = '102.16029']/cda:entry/cda:observation[@classCode='OBS']">


            <assert test="@moodCode = 'EVN'">Error: e-Referral - Other (Pathology or radiology)
                Test Result - The 'observation' tag 'moodCode' attribute shall contain the value
                'EVN'. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>


            <!-- code -->
            
            <assert test="cda:code or cda:originalText">Error: e-Referral - Other (Pathology or radiology) Test Result
                - The 'code' or 'originalText' tag is missing. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <report test="count(cda:code) > 1">Error: e-Referral - Other (Pathology or radiology)
                Test Result - "Pathology Test Result Name" - The 'code' tag shall appear only once.
                Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>
            
            <report test="count(cda:originalText) > 1">Error: e-Referral - Other (Pathology or radiology)
                Test Result - "Pathology Test Result Name" - The 'originalText' tag shall appear only once.
                Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>
  
            <!-- id -->
            <report test="count(cda:id) > 1">Error: e-Referral - Other (Pathology or radiology)
                Test Result - The 'id' tag shall appear only once. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>

          <!--effectiveTime-->
            <report test="count(cda:effectiveTime) > 1">Error: e-Referral - Other (Pathology or
                radiology) Test Result - The 'effectiveTime' tag shall appear only once. Refer to section
                of the e-Referral_CDA_Implementation_Guide_v2.2.</report>
            
            <!-- value:ED -->
            
            <assert test="cda:value">Error: e-Referral -  Other (Pathology or radiology) Test Result
                - The 'value' tag is missing. Refer
                to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>
            
            <report test="count(cda:value) > 1">Error: e-Referral - Other (Pathology or radiology) Test
                Result  - The 'value' tag shall appear only
                once. Refer to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>
            
            <report
                test="count(cda:entryRelationship[cda:observation/cda:code/@code='308552006']) > 1"
                >Error: e-Referral - Other (Pathology or radiology) Test Result - The
                'entryRelationship' tag shall appear only once. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>

        </rule>


        <rule
            context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '101.20117']/cda:component/cda:section[cda:code/@code = '102.16029']/cda:entry/cda:observation[@classCode='OBS']/cda:value">
            
            <assert test="@xsi:type = 'ED'">Error: e-Referral - Other (Pathology or radiology) Test Result
                - The 'value' tag 'xsi:type'
                attribute shall contain value 'ED'. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>
            

        </rule>
        
       
        <rule
            context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code='101.20117']/cda:component/cda:section[cda:code/@code='102.16029']/cda:entry/cda:observation[@classCode='OBS']/cda:entryRelationship[cda:observation/cda:code/@code='308552006']">

            <assert test="@typeCode = 'COMP'">Error: e-Referral - Other (Pathology or radiology)
                Test Result - The 'entryRelationship' tag 'typeCode' attribute shall contain value
                'COMP'. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <report test="count(cda:observation) > 1">Error: e-Referral - Other (Pathology or
                radiology) Test Result - The 'observation/code/@code="308552006"' tag shall appear
                only once. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>

        </rule>


        <rule
            context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code='101.20117']/cda:component/cda:section[cda:code/@code='102.16029']/cda:entry/cda:observation[@classCode='OBS']/cda:entryRelationship/cda:observation[cda:code/@code='308552006']">

            
            <assert test="@classCode">Error: e-Referral - Other (Pathology or radiology) Test Result 
                 - The 'observation' tag 'classCode' attribute
                is missing. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <assert test="@classCode = 'OBS'">Error: e-Referral - Other (Pathology or radiology) Test Result
                 - The 'observation' tag 'classCode'
                attribute shall contain value 'OBS'. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <assert test="@moodCode">Error: e-Referral - Other (Pathology or radiology) Test Result 
                 - The 'observation' tag 'moodCode' attribute is
                missing. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <assert test="@moodCode = 'EVN'">Error: e-Referral - Other (Pathology or radiology) Test Result /
                Overall Pathology Test Result Status" - The 'observation' tag 'moodCode' attribute
                shall contain value 'EVN'. Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <!-- code -->

            <report test="count(cda:code) > 1">Error: e-Referral - Other (Pathology or radiology) Test Result
                 - The 'code' tag shall appear only once.
                Refer to section  of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>

            <!-- value:CD -->
            
            <assert test="cda:value">Error: e-Referral -  Other (Pathology or radiology) Test Result
                - The 'value' tag is missing. Refer
                to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <report test="count(cda:value) > 1">Error: e-Referral - Other (Pathology or radiology) Test
                Result  - The 'value' tag shall appear only
                once. Refer to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>

        </rule>


        <rule
            context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code='101.20117']/cda:component/cda:section[cda:code/@code='102.16029']/cda:entry/cda:observation[@classCode='OBS']/cda:entryRelationship/cda:observation[cda:code/@code='308552006']/cda:value">

            <!-- value:CD -->

            <assert test="@code">Error: e-Referral -  Other (Pathology or radiology) Test Result 
                 - The 'value' tag 'code' attribute is missing. Refer
                to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <assert
                test="document('CDAValidate_Vocabs.xml')/systems/system[@codeSystemName = 'NCTIS_Admin_Codes_Result_Status']/code[(@code = current()/@code)]"
                >Error: e-Referral -  Other (Pathology or radiology) Test Result 
                 - The 'value' tag 'code' attribute shall be as per
                'NCTIS_Admin_Codes_Result_Status'. Refer to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <!-- @codeSystem -->

            <assert test="@codeSystem">Error: e-Referral -  Other (Pathology or radiology) Test Result 
                 - The 'value' tag 'codeSystem' attribute is
                missing. Refer to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <assert
                test="document('CDAValidate_Vocabs.xml')/systems/system[@codeSystemName = 'NCTIS_Admin_Codes_Result_Status']/code[(@codeSystem = current()/@codeSystem)]"
                >Error: e-Referral -  Other (Pathology or radiology) Test Result 
                 - The 'value' tag 'codeSystem' attribute shall be as per
                'NCTIS_Admin_Codes_Result_Status'. Refer to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <!-- @displayName -->

            <report
                test="@displayName and not(
                document('CDAValidate_Vocabs.xml')/systems/system[@codeSystemName = 'NCTIS_Admin_Codes_Result_Status']/code[
                (@code = current()/@code) and (translate(@displayName,  'abcdefghijklmnopqrstuvwxyz', 'ABCDEFGHIJKLMNOPQRSTUVWXYZ') = translate(current()/@displayName, 'abcdefghijklmnopqrstuvwxyz', 'ABCDEFGHIJKLMNOPQRSTUVWXYZ'))
                ])"
                >Error: e-Referral -  Other (Pathology or radiology) Test Result 
                 - The 'value' tag 'displayName' attribute shall be as per
                'NCTIS_Admin_Codes_Result_Status'. Refer to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>

            <!-- @xsi:type -->

            <assert test="@xsi:type = 'CD'">Error: e-Referral -  Other (Pathology or radiology) Test Result 
                 - The 'value' tag 'xsi:type' attribute shall
                contain the value 'CD'. Refer to section  and 8.1 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>
 

        </rule>
       
       
        <rule context = "cda:code[@code = '308552006' and ancestor::*[cda:code/@code = '102.16029']]">
        
            <assert test = "
                count(/cda:ClinicalDocument//cda:section[cda:code/@code = '102.16029']//cda:code[@code = '308552006']) =
                count(/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '101.20117']/
                cda:component/cda:section[cda:code/@code = '102.16029']/
                cda:entry/cda:observation[@classCode = 'OBS']/
                cda:entryRelationship/cda:observation/cda:code[@code = '308552006'])"
                >Error: e-Referral -  Other (Pathology or radiology) Test Result  - The
                "/ClinicalDocument/component/structuredBody/component/section[code/@code = '101.20117']/component/section[code/@code = '102.16145']/entry/observation[@classCode = 'OBS']/entryRelationship/observation/code[@code = '308552006']"
                path - 1 or more tags are missing.
                Refer section of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>
            
            <assert test = "@codeSystem = '2.16.840.1.113883.6.96'">Error: e-Referral -
                Other (Pathology or radiology) Test Result  -
                The 'code' tag 'codeSystem' attribute shall contain value '2.16.840.1.113883.6.96'.
                Refer to section 7.1.6.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>
            
            <assert test = "@codeSystemVersion = '20110531'">Error: e-Referral -
                Other (Pathology or radiology) Test Result  -
                The 'code' tag 'codeSystemVersion' attribute shall contain value '20110531'.
                Refer to section 7.1.6.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>
            
            <assert
                test = "translate(@codeSystemName, 'abcdefghijklmnopqrstuvwxyz', 'ABCDEFGHIJKLMNOPQRSTUVWXYZ') = 'SNOMED CT-AU'"
                >Error: e-Referral -  Other (Pathology or radiology) Test Result -
                The 'code' tag 'codeSystemName' attribute shall contain value 'SNOMED CT-AU'.
                Refer to section 7.1.6.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>
            
            <assert
                test = "translate(@displayName, 'abcdefghijklmnopqrstuvwxyz', 'ABCDEFGHIJKLMNOPQRSTUVWXYZ') = 'REPORT STATUS'"
                >Error: e-Referral -  Other (Pathology or radiology) Test Result  -
                The 'code' tag 'displayName' attribute shall contain value 'report status'.
                Refer to section of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>
            
        </rule>


    </pattern>


