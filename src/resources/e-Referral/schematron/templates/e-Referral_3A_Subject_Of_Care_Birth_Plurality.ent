

    <!-- e-Referral Clinical Document:  6.1.2 Subject of Care -->

    <!-- Context: ClinicalDocument//component/structuredBody/component[admin_obs]/section -->

    <!-- Status: Last Reviewed: 23/12/2011.
         Status: Last Updated:  26/04/2012 -->

    <pattern name="p-e-Referral_3A_Subject_Of_Care_Birth_Plurality-errors"
        id="p-e-Referral_3A_Subject_Of_Care_Birth_Plurality-errors"
        see="#p-e-Referral_3A_Subject_Of_Care_Birth_Plurality-errors">

        <!-- Birth Plurality - 0..1 -->

        <rule context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '102.16080']">

            <!-- entry - 0..1 -->

            <report test="count(cda:entry[cda:observation/cda:code/@code = '103.16249']) > 1">Error:
                e-Referral - 6.1.2 Subject of Care - "Subject of Care / Participant / Person or
                Organisation or Device / Person / Demographic Data / Birth Plurality" - The 'entry'
                tag shall appear only once. Refer to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>


            <!-- For 'entry' tag see below rule context:  
                <rule context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section/cda:entry[cda:observation/cda:code/@code = '103.16249']"> -->

        </rule>


        <!-- entry - 0..1 -->

        <rule context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '102.16080']/cda:entry[cda:observation/cda:code/@code = '103.16249']">

            <!-- observation - 1..1 -->
            
            <report test="count(cda:observation[cda:code/@code = '103.16249']) > 1">Error:
                e-Referral - 6.1.2 Subject of Care - "Subject of Care / Participant / Person or
                Organisation or Device / Person / Demographic Data / Birth Plurality" - The
                'observation' tag shall appear only once. Refer to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>


            <!-- For 'observation' tag see below rule context:  
                <rule context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section/cda:entry/cda:observation[cda:code/@code = '103.16249']"> -->

        </rule>


        <!-- observation - 1..1 -->

        <rule context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '102.16080']/cda:entry/cda:observation[cda:code/@code = '103.16249']">

            <!-- @classCode -->

            <assert test="@classCode">Error: e-Referral - 6.1.2 Subject Of Care - "Subject of Care /
                Participant / Person or Organisation or Device / Person / Demographic Data / Birth
                Plurality" - The 'observation' tag 'classCode' attribute is missing. Refer to
                section 6.1.2 of the e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <assert test="@classCode = 'OBS'">Error: e-Referral - 6.1.2 Subject Of Care - "Subject
                of Care / Participant / Person or Organisation or Device / Person / Demographic Data
                / Birth Plurality" - The 'observation' tag 'classCode' attribute shall contain the value 'OBS'.
                Refer to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <!-- @moodCode -->

            <assert test="@moodCode">Error: e-Referral - 6.1.2 Subject Of Care - "Subject of Care /
                Participant / Person or Organisation or Device / Person / Demographic Data / Birth
                Plurality" - The 'observation' tag 'moodCode' attribute is missing. Refer to section
                6.1.2 of the e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <assert test="@moodCode = 'EVN'">Error: e-Referral - 6.1.2 Subject Of Care - "Subject of
                Care / Participant / Person or Organisation or Device / Person / Demographic Data /
                Birth Plurality" - The 'observation' tag 'moodCode' attribute shall contain the value 'EVN'. Refer
                to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <!-- code -->
            
            <report test="count(cda:code[@code = '103.16249']) > 1">Error: e-Referral - 6.1.2
                Subject of Care - "Subject of Care / Participant / Person or Organisation or Device
                / Person / Demographic Data / Birth Plurality" - The 'code' tag shall appear only
                once. Refer to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>


            <!-- For 'code' tag see below rule context:  <rule context="cda:code[@code = '103.16249']"> -->


            <!-- id - 1..1 -->

            <assert test="cda:id">Error: e-Referral - 6.1.2 Subject Of Care - "Subject of Care /
                Participant / Person or Organisation or Device / Person / Demographic Data / Birth
                Plurality" - The 'id' tag is missing. Refer to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <report test="count(cda:id) > 1">Error: e-Referral - 6.1.2 Subject Of Care - "Subject of
                Care / Participant / Person or Organisation or Device / Person / Demographic Data /
                Birth Plurality" - The 'id' tag shall appear only once. Refer to section 6.1.2 of
                the e-Referral_CDA_Implementation_Guide_v2.2.</report>

            <!-- The 'id' tag is being tested by "e-Referral_Global_Checks" ent file. -->


            <!-- value - 1..1 -->

            <assert test="cda:value">Error: e-Referral - 6.1.2 Subject Of Care - "Subject of Care /
                Participant / Person or Organisation or Device / Person / Demographic Data / Birth
                Plurality" - The 'value' tag is missing. Refer to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <report test="count(cda:value) > 1">Error: e-Referral - 6.1.2 Subject Of Care - "Subject
                of Care / Participant / Person or Organisation or Device / Person / Demographic Data
                / Birth Plurality" - The 'value' tag shall appear only once. Refer to section 6.1.2
                of the e-Referral_CDA_Implementation_Guide_v2.2.</report>


            <!-- For 'value' tag see below rule context:  
                <rule context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section/cda:entry/cda:observation[cda:code/@code = '103.16249']/cda:value"> -->

        </rule>


        <!-- code -->

        <rule context="cda:code[@code = '103.16249']">

            <!-- checking parent path -->

            <assert test="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '102.16080']/cda:entry/cda:observation/cda:code[@code = '103.16249']"
                >Error: e-Referral - 6.1.2 Subject Of Care - 
                The '/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[code/@code = '102.16080']/cda:entry/cda:observation/cda:code[@code = '103.16249']' path - 
                1 or more tags are missing. Refer to section 6.1.2 of the e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <!-- @codeSystem -->

            <assert test="@codeSystem = '1.2.36.1.2001.1001.101'">Error: e-Referral - 6.1.2 Subject
                Of Care - "Subject of Care / Participant / Person or Organisation or Device / Person
                / Demographic Data / Birth Plurality" - The 'code' tag 'codeSystem' attribute shall
                be '1.2.36.1.2001.1001.101'. Refer to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <!-- @codeSystemName -->

            <assert
                test="translate(@codeSystemName, 'abcdefghijklmnopqrstuvwxyz', 'ABCDEFGHIJKLMNOPQRSTUVWXYZ') = 'NCTIS DATA COMPONENTS'"
                >Error: e-Referral - 6.1.2 Subject Of Care - "Subject of Care / Participant / Person
                or Organisation or Device / Person / Demographic Data / Birth Plurality" - The
                'code' tag 'codeSystemName' attribute shall contain the value 'NCTIS Data Components'. Refer to
                section 6.1.2 of the e-Referral_CDA_Implementation_Guide_v2.2.</assert>

            <!-- @displayName -->

            <assert
                test="translate(@displayName, 'abcdefghijklmnopqrstuvwxyz', 'ABCDEFGHIJKLMNOPQRSTUVWXYZ') = 'BIRTH PLURALITY'"
                >Error: e-Referral - 6.1.2 Subject Of Care - "Subject of Care / Participant / Person
                or Organisation or Device / Person / Demographic Data / Birth Plurality" - The
                'code' tag 'displayName' attribute shall contain the value 'Birth Plurality'. Refer to section
                6.1.2 of the e-Referral_CDA_Implementation_Guide_v2.2.</assert>

        </rule>


        <!-- value - 1..1 -->

        <rule context="/cda:ClinicalDocument/cda:component/cda:structuredBody/cda:component/cda:section[cda:code/@code = '102.16080']/cda:entry/cda:observation[cda:code/@code = '103.16249']/cda:value">

            <!-- @xsi:type -->
            <!-- Start Mantis Task 2782 -->         
            <assert test="@xsi:type">
                Error: e-Referral - 6.1.2 Subject Of
                Care - "Subject of Care / Participant / Person or Organisation or Device / Person /
                Demographic Data / Birth Plurality" - The 'value' tag 'xsi:type' attribute is missing. 
                Refer to section 6.1.2 of the e-Referral_CDA_Implementation_Guide_v2.2.
            </assert>
            
            <assert test="not(@xsi:type) or normalize-space(@xsi:type) !=  ''">
                Error: e-Referral - 6.1.2 Subject Of
                Care - "Subject of Care / Participant / Person or Organisation or Device / Person /
                Demographic Data / Birth Plurality" - The 'value' tag 'xsi:type' attribute shall contain a value. 
                Refer to section 6.1.2 of the e-Referral_CDA_Implementation_Guide_v2.2.
            </assert>
            
            <assert test="normalize-space(@xsi:type) = 'INT'">
                Error: e-Referral - 6.1.2 Subject Of
                Care - "Subject of Care / Participant / Person or Organisation or Device / Person /
                Demographic Data / Birth Plurality" - The 'value' tag 'xsi:type' attribute shall contain the value 'INT'. 
                Refer to section 6.1.2 of the e-Referral_CDA_Implementation_Guide_v2.2.
            </assert>
            
            <!--<report test="@xsi:type and @xsi:type !=  'INT'">Error: e-Referral - 6.1.2 Subject Of
                Care - "Subject of Care / Participant / Person or Organisation or Device / Person /
                Demographic Data / Birth Plurality" - The 'value' tag 'xsi:type' attribute shall
                contain the value 'INT'. Refer to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</report>-->
            <!-- End Mantis Task 2782 -->
            
            <!-- @value -->

            <assert test="number(@value)">Error: e-Referral - 6.1.2 Subject Of Care - "Subject of
                Care / Participant / Person or Organisation or Device / Person / Demographic Data /
                Birth Plurality" - The 'value' tag 'value' attribute shall contain a value that is a
                positive integer greater than or equal to 1. Refer to section 6.1.2 of the
                e-Referral_CDA_Implementation_Guide_v2.2.</assert>


        </rule>

    </pattern>
